{
    "name": "akarah/hprim",
    "description": "HPRIM parser, generator and sender.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AkarahHPRIM",
            "email": "hello@akarah.com",
            "role": "Owner"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "php": ">=7.3",
        "ext-sockets": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "dms/phpunit-arraysubset-asserts": "^0.2",
        "ext-pcntl": "*"
    },
    "autoload": {
        "psr-4": {
            "Akarah\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akarah\\HL7\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
